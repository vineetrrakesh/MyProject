projection;
use draft;

define behavior for ZC_RAP_TRAVEL_VINEE alias Travel
//use etag
{
  use create;
  use update;
  use delete;

  use association _Booking { create; with draft; }

  use action acceptTravel;
  use action rejectTravel;
}

define behavior for ZC_RAP_Booking_VINEE alias Booking
//use etag
{
  use update;
  use delete;

  use association _Travel { with draft; }
}